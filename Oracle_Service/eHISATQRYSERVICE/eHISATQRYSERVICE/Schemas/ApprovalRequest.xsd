<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="ActionRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Target" type="xs:string" minOccurs="0" />
        <xs:element name="Identifier" type="xs:string" minOccurs="0" />
        <xs:element name="LoginID" type="xs:string" minOccurs="0" />
        <xs:element name="Action" type="xs:string" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ApprovalRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="TransactionID" type="xs:string" minOccurs="0" />
        <xs:element name="ProcessID" type="xs:string" minOccurs="0" />
		<xs:element name="InPatientNo" type="xs:string" minOccurs="0" />
  	    <xs:element name="InputParam" minOccurs="0" maxOccurs="unbounded">
		</xs:element>
		<xs:element name="ApprovalStatus" type="xs:string" minOccurs="0" />
  	    <xs:element name="ApprovalRole" type="xs:string" minOccurs="0" />
		<xs:element name="ApprovalLevel" type="xs:string" minOccurs="0" />
		<xs:element name="Remarks" type="xs:string" minOccurs="0" />
		<xs:element ref="ActionRequest" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="ActionRequest" />
        <xs:element ref="ApprovalRequest" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>